{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "react-client",
            "image": "knsakib/react-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "express-api-server",
            "image": "knsakib/express-api-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx-router",
            "image": "knsakib/nginx-router",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostport": 80,
                    "containerPort": 80
                }
            ],
            "links": ["react-client", "express-api-server"]
        },
        {
            "name": "fib-worker",
            "image": "knsakib/fib-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        }
    ]
}